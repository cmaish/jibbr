<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="Bot.Name" value="username" />
    <add key="Bot.Password" value="password" />
    <add key="Bot.Server" value="http://jabbr.lvh.me/" />
    <add key="Bot.RoomList" value="test" />
    <add key="Application.Gravatar" value="" />
    <!--<add key="Twitter.ConsumerKey" value="" />
    <add key="Twitter.ConsumerSecret" value="" />
    <add key="Twitter.Token" value="" />
    <add key="Twitter.TokenSecret" value="" />
    <add key="Twitter.UserName" value="" />-->
    <add key="Blogs.FeedList" value="@code_52,http://code52.org/rss.xml" />
    <add key="Blogs.HoursOldThreshold" value="48" />
  </appSettings>
  <system.diagnostics>
    <sources>
      <source name="System.ComponentModel.Composition"
      switchValue="All">
        <listeners>
          <add name="fileListener"
          type="System.Diagnostics.TextWriterTraceListener"
          initializeData="composition.log" />
        </listeners>
      </source>
    </sources>
    <trace autoflush="true" indentsize="4" />
  </system.diagnostics>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.0.5.0" newVersion="4.0.5.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>